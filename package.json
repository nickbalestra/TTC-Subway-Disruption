{
  "name": "ttc-subway-disruptions",
  "version": "1.0.0",
  "description": "A simple tool to display, add and remove disruptions on the 4 metro lines in Toronto operated by the TCC",
  "main": "app.js",
  "scripts": {
    "dev": "webpack --watch",
    "test": "jest"
  },
  "author": "Nick Balestra",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^3.3.6",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "superagent": "^1.7.2"
  },
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-jest": "*",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-register": "^6.4.3",
    "css-loader": "^0.23.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "jest-cli": "*",
    "jest-webpack-alias": "^2.2.0",
    "react-addons-test-utils": "~0.14.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testPathIgnorePatterns": ["/fixtures/", "/node_modules/"],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
